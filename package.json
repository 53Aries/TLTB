{
  "name": "tltbmobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "export:ble-schema": "ts-json-schema-generator --tsconfig tsconfig.json --path src/types/device.ts --type BleStatusPayload --out docs/ble_payload_schema.json",
    "validate:ble-payload": "node scripts/validateBlePayload.js",
    "decode:ble-payload": "node scripts/decodeBlePayload.js",
    "test": "vitest run"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "base-64": "^1.0.0",
    "expo": "~54.0.33",
    "expo-dev-client": "~6.0.20",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-ble-plx": "^3.5.0",
    "react-native-safe-area-context": "^5.6.2",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.19.32",
    "@types/react": "~19.1.0",
    "ajv": "^8.17.1",
    "ts-json-schema-generator": "^1.5.1",
    "typescript": "~5.9.2",
    "vitest": "^1.6.1"
  },
  "private": true
}
